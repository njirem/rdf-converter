// Place your settings in this file to overwrite default and user settings.
{
	"editor.insertSpaces": true,
	"editor.tabSize": 4,
    "editor.formatOnType": true,
    "files.trimTrailingWhitespace": true,
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"src/**/*.js": true,
		"src/**/*.js.map": true
	},
    "telemetry.enableCrashReporter": false
}